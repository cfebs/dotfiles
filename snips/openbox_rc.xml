<keybind key="W-r">
  <action name="Execute">
    <startupnotify>
      <enabled>true</enabled>
      <name>dmenu</name>
    </startupnotify>
    <command>dmenu_run -l 10 -fn '-misc-fixed-medium-r-normal--18-*-*-*-*-*-*-*'</command>
  </action>
</keybind>
<keybind key="W-p">
  <action name="Execute">
    <startupnotify>
      <enabled>true</enabled>
      <name>display</name>
    </startupnotify>
    <command>xfce4-display-settings -m</command>
  </action>
</keybind>
<keybind key="W-t">
  <action name="Execute">
    <startupnotify>
      <enabled>true</enabled>
      <name>display</name>
    </startupnotify>
    <command>
      <execute>x-terminal-emulator</execute>
    </command>
  </action>
</keybind>
<keybind key="W-w">
  <action name="Execute">
    <startupnotify>
      <enabled>true</enabled>
      <name>display</name>
    </startupnotify>
    <command>
      <execute>sensible-browser</execute>
    </command>
  </action>
</keybind>
<keybind key="W-f">
  <action name="Execute">
    <startupnotify>
      <enabled>true</enabled>
      <name>display</name>
    </startupnotify>
    <command>
      <execute>thunar</execute>
    </command>
  </action>
</keybind>
